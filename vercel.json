{
    "version": 2,
    "builds": [
      {
        "src": "lyf-admin/package.json",
        "use": "@vercel/node",
        "config": {
          "runtime": "nodejs",
          "engines": {
            "node": "22"
          },
          "buildCommand": "npm run build:prod",
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js"
      }
    ]
  }